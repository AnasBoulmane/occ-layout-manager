{
  "name": "occ-layout-manager",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.pack.json",
    "test": "jest --forceExit --coverage --verbose",
    "coveralls": "jest --forceExit --coverage --coverageReporters=text-lcov | coveralls",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"npm run watch-node\"",
    "watch-node": "npm run build && nodemon --delay 1 dist/index.js -k $KPROD -n $NPROD  -r wi1400001 --selectedInstance wi300023",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "format": "prettier-tslint fix \"./**/*.ts\"",
    "prepack": "echo \"prepack...\" && yarn build"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "commander": "^3.0.2",
    "listr": "^0.14.3",
    "qs": "^6.9.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/listr": "^0.14.2",
    "@types/node": "^12.7.12",
    "@types/qs": "^6.5.3",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/index.d.ts"
  ],
  "tags": [
    "cli",
    "occ",
    "oracle",
    "typescript",
    "unit-testing",
    "commerce-cloud"
  ],
  "keywords": [
    "cli",
    "occ",
    "oracle",
    "typescript",
    "unit-testing",
    "commerce-cloud"
  ]
}
