{
  "name": "occ-layout-manager",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.pack.json",
    "test": "jest --forceExit --coverage --verbose",
    "coveralls": "jest --forceExit --coverage --coverageReporters=text-lcov | coveralls",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"npm run watch-node\"",
    "watch-node": "npm run build && nodemon --delay 1 dist/index.js -k $KSTAGR -n $NSTAGE",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "format": "prettier-tslint fix \"./**/*.ts\"",
    "prepack": "echo \"prepack...\" && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": "prettier-tslint fix"
  },
  "dependencies": {
    "inquirer-autocomplete-prompt": "latest",
    "commander": "^3.0.2",
    "inquirer": "^7.0.0",
    "axios": "^0.19.0",
    "listr": "^0.14.3",
    "rxjs": "^6.5.3",
    "ora": "latest",
    "qs": "^6.9.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.19",
    "@types/listr": "^0.14.2",
    "@types/node": "^12.11.7",
    "@types/qs": "^6.5.3",
    "axios-retry": "^3.1.2",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.6",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-mock-axios": "^3.1.2",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/index.d.ts"
  ],
  "keywords": [
    "cli",
    "occ",
    "oracle",
    "typescript",
    "unit-testing",
    "commerce-cloud"
  ]
}
